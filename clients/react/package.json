{
  "name": "sample-todo-react",
  "version": "0.0.1",
  "description": "Sample react application.",
  "scripts": {
    "prestart": "npm run remove-dist",
    "start": "parallelshell \"npm run lint:watch\" \"npm run test:watch\" \"npm run open:src\"",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint:tools": "eslint webpack.config.js tools",
    "lint": "eslint src tools || echo Look at eslint errors above",
    "lint:fix": "eslint src tools --fix",
    "lint:watch": "watch \"npm run lint\" src tools",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm run clean-dist && npm run build:html",
    "build": "npm run test && babel-node tools/build.js && npm run open:dist",
    "test": "cross-env NODE_ENV=test mocha --reporter progress --compilers js:babel-core/register --recursive \"./src/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Alexander Boychenko",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "6.7.4",
    "bootstrap-sass": "3.3.6",
    "jquery": "2.2.2",
    "object-assign": "4.0.1",
    "react": "0.14.7",
    "react-bootstrap": "0.28.3",
    "react-dom": "0.14.7",
    "react-redux": "4.4.1",
    "react-router": "2.0.1",
    "react-router-bootstrap": "0.20.1",
    "redux": "3.3.1",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.0.1",
    "superagent": "1.8.2"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-core": "6.7.4",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-react-transform": "2.0.2",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "bootstrap-loader": "1.0.10",
    "browser-sync": "2.11.2",
    "chai": "3.5.0",
    "cheerio": "0.20.0",
    "colors": "1.1.2",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "eslint": "2.4.0",
    "eslint-config-airbnb": "6.2.0",
    "eslint-loader": "1.3.0",
    "eslint-plugin-react": "4.2.3",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "imports-loader": "0.6.5",
    "mocha": "2.4.5",
    "node-sass": "3.4.2",
    "parallelshell": "2.0.0",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "redbox-react": "1.2.2",
    "resolve-url-loader": "1.4.3",
    "rimraf": "2.5.2",
    "sass-loader": "3.2.0",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "watch": "0.17.1",
    "webpack": "1.12.14",
    "webpack-dev-middleware": "1.5.1",
    "webpack-hot-middleware": "2.10.0",
    "yargs": "4.3.2"
  }
}
